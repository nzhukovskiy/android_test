<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".third_fragment.Third_fragment">

    <TextView
        android:id="@+id/third_fragment_text"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintTop_toTopOf="parent"
        android:text="Hello" />
    <TextView
        android:id="@+id/third_fragment_country"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@+id/third_fragment_text"
        android:text="Country" />
    <TextView
        android:id="@+id/third_fragment_population"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@+id/third_fragment_country"
        android:text="Population" />

</androidx.constraintlayout.widget.ConstraintLayout>